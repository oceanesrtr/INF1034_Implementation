<?xml version="1.0" encoding="UTF-8"?>
<!--fxml de l'interface proprietaire : section modification de menu-->
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<Pane maxWidth="624.0" prefWidth="624.0" style="-fx-padding: 40 0 0 40; -fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.delicivite.proprietaire.ControllerModifierMenu">
    <!--Lien avec le fichier CSS correspondant-->
    <stylesheets>
        <URL value="@proprietaire.css" />
    </stylesheets>
    <!--[1] : Contenu de la sous fenetre -->
    <children>

        <VBox fillWidth="false" layoutX="22.0" layoutY="37.0" maxWidth="600.0" prefWidth="400.0" spacing="15.0">
            <Text fx:id="titre1" text="Ajouter un groupe">
            <font>
               <Font size="20.0" />
            </font></Text>
            <TextField fx:id="nouveauGroupe" prefHeight="28.0" prefWidth="267.0" style="-fx-background-color: #FBFAFF; -fx-border-color: #424242; -fx-border-radius: 15;" VBox.vgrow="NEVER" />
            <Button maxHeight="25.0" maxWidth="267.0" onAction="#onAjoutGroupe" prefHeight="25.0" prefWidth="267.0" styleClass="boutonAction" text="Ajouter">
            <VBox.margin>
               <Insets bottom="5.0" />
            </VBox.margin></Button>
         <Line endX="309.29290771484375" endY="-1.449711799621582" opacity="0.25" startX="-100.0" />

            <Text fx:id="titre2" text="Supprimer un groupe">
            <font>
               <Font size="20.0" />
            </font></Text>
            <ChoiceBox fx:id="groupe" prefHeight="28.0" prefWidth="273.0" style="-fx-background-color: #FBFAFF; -fx-border-color: #424242; -fx-border-radius: 15;"> </ChoiceBox>
            <Button maxHeight="25.0" maxWidth="273.0" onAction="#onSupprimerGroupe" prefHeight="25.0" prefWidth="273.0" styleClass="boutonAction" text="Supprimer"> Supprimer </Button>
        </VBox>
    </children>

</Pane>
